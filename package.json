{
  "name": "mye-postage-service",
  "type": "module",
  "version": "1.1.0",
  "scripts": {
    "dev": "bun run --hot src/index.ts --console-depth=10",
    "lint": "bunx @biomejs/biome check",
    "typecheck": "tsc --noEmit",
    "format": "bunx @biomejs/biome format --write",
    "prepare": "husky",
    "db:local": "bunx prisma dev",
    "db:migrate": "bunx prisma migrate dev",
    "db:studio": "bunx prisma studio",
    "db:reset": "bunx prisma migrate reset --force",
    "db:generate": "bunx prisma generate",
    "db:seed": "bunx prisma db seed"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.842.0",
    "@hono/zod-openapi": "^0.19.2",
    "@prisma/adapter-pg": "^6.13.0",
    "@prisma/client": "^6.11.0",
    "@scalar/hono-api-reference": "^0.5.162",
    "bullmq": "^5.56.1",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "hono": "^4.8.5",
    "hono-pino": "^0.7.0",
    "husky": "^9.1.7",
    "ioredis": "^5.6.1",
    "p-limit": "^6.2.0",
    "p-queue": "^8.1.0",
    "pdf-lib": "^1.17.1",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "puppeteer": "^24.11.2",
    "stoker": "^1.4.2",
    "xior": "^0.7.8",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/bun": "^1.2.19",
    "prisma": "^6.13.0",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.8.3"
  }
}
